{
  "name": "submerge",
  "version": "1.1.0",
  "description": "Recursively merge various objects into a single new object, optionally capable of reflecting changes on the merged object ",
  "main": "lib/submerge.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "posttest": "./node_modules/.bin/nsp check",
    "test": "./node_modules/.bin/lab -f -t 100 -m 3000",
    "test-coverage": "./node_modules/.bin/lab -f -r html -m 3000 -o report.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/konfirm/node-submerge.git"
  },
  "keywords": [
    "merge",
    "recursive",
    "combine"
  ],
  "author": "Rogier Spieker",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/konfirm/node-submerge/issues"
  },
  "homepage": "https://github.com/konfirm/node-submerge",
  "devDependencies": {
    "code": "^2.1.0",
    "coveralls": "^2.11.6",
    "lab": "^8.0.2",
    "nsp": "^2.2.0"
  },
  "engines": {
    "node": ">=4"
  }
}
